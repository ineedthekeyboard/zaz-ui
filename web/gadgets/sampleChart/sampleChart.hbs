<div class="container zaz-effect zaz-effect-bar-top">
    <div class="controls">
        <div class="controls-bar">
            <div class="message">
                {{items.APPLICATIONS-DATA-1}}
            </div>
        </div>
        <div class="controls-bar">
            <div class="knobs">
                <div tabindex="0" class="knob active" data-view="columns" title="View as Columns Chart">
