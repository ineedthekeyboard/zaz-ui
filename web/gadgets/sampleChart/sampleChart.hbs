                    <label>Columns</label>
                </div>
                <div tabindex="0" class="knob"  data-view="pie" title="View as Pie Chart">
                    <i class="material-icons md-light">pie_chart</i>
                    <label>Pie</label>
                </div>
            </div>
        </div>
    </div>
    <div class="chart"></div>
